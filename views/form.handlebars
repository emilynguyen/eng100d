
<h1>Edit Market Assessment Form</h1>

  <form id="assessment-form" onsubmit="save(); return false;">
    <ol id="question-list" class="sortable">
      {{#each questions}}
        {{> question}}
      {{/each}}
    </ol>

    <input type="submit" value="Save">
  </form>

