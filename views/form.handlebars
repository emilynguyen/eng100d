
<h1>Edit Market Assessment Form</h1>

  <form id="assessment-form" onsubmit="save(); return false;">
    <ol id="question-list" class="sortable">
      {{#each questions}}
        {{> question}}
      {{/each}}
    </ol>

    <button type="submit">Save</button>
  </form>

  <button type="button" class="add-question-btn" onclick="addQuestion()">Add Question</button>

