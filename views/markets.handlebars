
<div class="container main">
    <div class="card">
        <div class="card-body">
            <table id="table_id" class="display">
                <thead>
                    <tr>
                        <th>Market Name</th>
                        <th>Store Type</th> 
                        <th>Zip Code</th>
                        <th>Level</th>
                    </tr>
                </thead>
                <tbody>
                    {{#each markets}}
                    <tr>
                        <td>{{name}}</td>

                        <td>{{storeType}}</td>

                        <td>{{address.zip}}</td>

                        <td>{{level}}</td>
                    </tr>
                    {{/each}}
                </tbody>
            </table>
        </div>
    </div>
</div>

<script type="text/javascript" src="js/markets.js"></script>
